            <i class=""></i>
            <p class="subtitle">Member since <%= user.neiconn.enrollDate.getMonth() + 1 %>  <%= user.neiconn.enrollDate.getFullYear() %></p>

     		<h6 class="title">Language</h6>
            
            
              <% if (user.neiconn.language) { %>
                    <p class="subtitle">
                        <%= user.neiconn.language %>
                    </p>
                <% } else { %>
                    <p class="subtitle">
                         Please Write Your language
                    </p>
                <% } %>


            
            <h6 class="title">School</h6>
            
               <% if (user.neiconn.school) { %>
                    <p class="subtitle">
                        <%= user.neiconn.school %>
                    </p>
                <% } else { %>
                    <p class="subtitle">
                         Please Write Your school
                    </p>
                <% } %>

             

          
            <h6 class="title">Work</h6>
            <p class="subtitle">
             
             <% if (user.neiconn.work) { %>
                    <p class="subtitle">
                        <%= user.neiconn.work %>
                    </p>
                <% } else { %>
                    <p class="subtitle">
                         Please Write Your work Place
                    </p>
                <% } %>  
             
            </p>

            <h6 class="titleOne">About Me</h6>
            <p class="subtitle">
               
             <% if (user.neiconn.about) { %>
